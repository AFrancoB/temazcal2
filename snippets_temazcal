n << step [0,1,2,3,4,5] $ lfsaw [cps/6];

zoom (0.5+(ebeat*0.05)) $ spin ((1/ebeat) + (etime*0.05)) $ tile [4,4] $ ((n==0)*u)+:((n==1)*v)+:((n==2)*w)+:((n==3)*x)+:((n==4)*y)+:((n==5)*z) >> video;

(1/ebeat)*0.125 >> fdbk;

